<html>
<head>
<title>Fibonacci series</title>
</head>
<body bgcolor="orange">
<script type="text/javascript">
var n=prompt("Enter the number","");
for(var i=1;i<=n;i++)
{
   document.write(fib(i)+"<BR/>");
}
documnet.close();
function fib(n)
{
   if(n==1) return 0;
   if(n==2) return 1;
   return fib(n-1)+fib(n-2);
}
</script>
</body>
</html>
